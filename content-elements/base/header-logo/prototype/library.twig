{% apply spaceless %}
    {% set elementId = elementId ?: 'header-logo-XvovFj' %}
    {% set imagePartId = imagePartId ?: 'header-logo-part-p94CZB' %}
    {% set logoSrc = logoSrc ?: bsi_cx_asset('../../../../static/image.png') %}
    {% set logoWidth = (logoWidth ?: properties.logoWidth) ?: 90 %}

    <div data-bsi-element="{{ elementId }}">
        <img data-bsi-element-part="{{ imagePartId }}" src="{{ logoSrc }}" width="{{ logoWidth }}" alt="Logo" style="margin: 0; display: inline-block; width: {{ logoWidth }}px; max-width: 100%; height: auto;" />
    </div>
{% endapply %}